{
  "name": "amplify-launcher",
  "productName": "Amplify Launcher",
  "version": "1.0.0",
  "description": "Amp Roleplay FiveM Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=build/amp_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Amplify Launcher\"",
    "create-installer-win": "node installer/windows.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1"
  },
  "build": {
    "files": [
      "build/*"
    ],
    "appId": "com.amplifylauncher.app"
  },
  "dependencies": {
    "electron-winstaller": "^4.0.0",
    "fs-extra": "^9.0.0",
    "jquery": "^3.4.1",
    "update-electron-app": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmpRoleplay/AmpRoleplayClient.git"
  }
}
