{
  "name": "amplify-launcher",
  "productName": "Amplify Launcher",
  "version": "1.4.0",
  "description": "Amp Roleplay FiveM Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=build/amp_icon.ico --prune=true --out=release-builds --version-string.ProductName=\"Amplify Launcher\"",
    "create-installer-win": "node installer/windows.js"
  },
  "build": {
    "appId": "com.amplifylauncher.app"
  },
  "author": "Amp Devs",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.2.0"
  },
  "dependencies": {
    "download-file": "^0.1.5",
    "electron-packager": "^14.2.1",
    "electron-winstaller": "^4.0.0",
    "fs-extra": "^9.0.0",
    "jquery": "^3.5.0",
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmpRoleplay/AmpRoleplayClient.git"
  }
}
